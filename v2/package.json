{
  "name": "amoya",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.2.1",
    "@material-ui/icons": "^4.2.1",
    "ace-builds": "^1.3.3",
    "asciidoctor": "^2.0.3",
    "debug": "^4.1.1",
    "dropbox": "^4.0.28",
    "highlightjs": "^9.12.0",
    "jquery": "^3.3.1",
    "jquery-ui-dist": "^1.12.1",
    "js-beautify": "^1.10.1",
    "lodash": "^4.17.11",
    "mdi-material-ui": "^6.2.0",
    "mobx": "^5.6.0",
    "mobx-react": "^6.1.1",
    "mobx-utils": "^5.4.1",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-scripts-ts": "3.1.0",
    "systemjs": "^4.1.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./node_modules --include-path ./src/ src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./node_modules --include-path ./src src/ -o src/ --watch --recursive",
    "build-worker": "tsc --module system --moduleResolution Node --outFile ./public/assets/worker/asciidoc-compiler-worker.js ./src/document/asciidoc-compiler-worker.ts",
    "watch-worker": "npm run build-worker -- --watch",
    "start-js": "react-scripts-ts start",
    "start": "npm-run-all -p watch-css watch-worker start-js",
    "build": "npm run build-css && npm run build-worker && react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/ace": "^0.0.42",
    "@types/debug": "^4.1.4",
    "@types/jest": "^24.0.15",
    "@types/jquery": "^3.3.22",
    "@types/lodash": "^4.14.118",
    "@types/node": "^12.6.8",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.9",
    "node-sass-chokidar": "^1.3.5",
    "npm-run-all": "^4.1.3",
    "typescript": "^3.1.6"
  }
}
